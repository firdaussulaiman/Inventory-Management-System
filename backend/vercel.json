{
	"version": 2,
	"builds": [
	  {
		"src": "api/**/*.js",
		"use": "@vercel/node"
	  }
	],
	"routes": [
	  {
		"src": "/api/users/(.*)",
		"dest": "/api/users.js"
	  },
	  {
		"src": "/api/assets/(.*)",
		"dest": "/api/assets.js"
	  },
	  {
		"src": "/api/contactus/(.*)",
		"dest": "/api/contactus.js"
	  }
	],
	"env": {
	  "MONGO_URI": "@mongo_uri",
	  "JWT_SECRET": "@jwt_secret",
	  "FRONTEND_URL": "@frontend_url",
	  "EMAIL_HOST": "@email_host",
	  "EMAIL_USER": "@email_user",
	  "EMAIL_PASS": "@email_pass"
	}
  }
  